{
  "name": "rabbit-browser",
  "version": "1.0.0",
  "description": "Browser automation tool for detecting interactive elements on web pages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test": "ts-node src/test-consent-sites.ts",
    "prepublishOnly": "npm run build",
    "example:simple": "ts-node src/examples/simple-usage.ts",
    "example:consent": "ts-node src/examples/consent-detection.ts"
  },
  "keywords": [
    "puppeteer",
    "highlight",
    "web-scraping",
    "consent-buttons",
    "interactive-elements",
    "browser-automation"
