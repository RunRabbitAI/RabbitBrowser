  "author": "",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^24.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/rabbit-browser.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/rabbit-browser/issues"
  },
  "homepage": "https://github.com/yourusername/rabbit-browser#readme"
